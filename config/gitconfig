[remote "origin"]
	url = git://github.com/gitster/git.git
	fetch = +refs/heads/*:refs/remotes/origin/*
	fetch = +refs/notes/amlog:refs/notes/amlog
[remote "github"]
	url = git@github.com:peff/git.git
	fetch = +refs/heads/*:refs/remotes/github/*
[remote]
	pushDefault = github
[rerere]
	enabled = true
	autoUpdate = true
[merge]
	conflictStyle = diff3
[test "default"]
	command = make -j8 test
[test "compile"]
        command = make -j8
