[remote "origin"]
	url = https://github.com/gitster/git.git
	fetch = +refs/heads/*:refs/remotes/origin/*
[remote "github"]
	url = git@github.com:peff/git.git
	fetch = +refs/heads/*:refs/remotes/github/*
[remote]
	pushDefault = github
[rerere]
	enabled = true
	autoUpdate = true
[merge]
	conflictStyle = diff3
[test "default"]
	command = make -j16 strict=yes test
[test "compile"]
        command = make -j16 strict=yes
